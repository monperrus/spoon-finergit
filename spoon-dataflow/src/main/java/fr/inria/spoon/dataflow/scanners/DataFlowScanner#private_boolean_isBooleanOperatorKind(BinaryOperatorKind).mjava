/** 
 * Checks if the return value of the binary operator is boolean.
 */
private
boolean
isBooleanOperatorKind
(
BinaryOperatorKind
kind
)
{
switch
(
kind
)
{
case
OR
:
case
AND
:
case
EQ
:
case
NE
:
case
LT
:
case
GT
:
case
LE
:
case
GE
:
case
INSTANCEOF
:
return
true
;
default
:
return
false
;
}
}
