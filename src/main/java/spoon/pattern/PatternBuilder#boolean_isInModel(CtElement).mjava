/** 
 * @param element to be checked element
 * @return true if element `element` is a template or a child of template
 */
boolean
isInModel
(
CtElement
element
)
{
if
(
element
!=
null
)
{
for
(
CtElement
patternElement
:
patternModel
)
{
if
(
element
==
patternElement
||
element
.
hasParent
(
patternElement
)
)
{
return
true
;
}
}
}
return
false
;
}
