/** 
 * Gets the simple name for a given Java qualified name.
 */
protected
String
getSimpleName
(
String
qualifiedName
)
{
if
(
hasInnerType
(
qualifiedName
)
>
0
)
{
return
qualifiedName
.
substring
(
hasInnerType
(
qualifiedName
)
+
1
)
;
}
else
if
(
hasPackage
(
qualifiedName
)
>
0
)
{
return
qualifiedName
.
substring
(
hasPackage
(
qualifiedName
)
+
1
)
;
}
else
{
return
qualifiedName
;
}
}
