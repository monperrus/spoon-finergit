@Override
public
void
visitCtCompilationUnit
(
CtCompilationUnit
compilationUnit
)
{
switch
(
compilationUnit
.
getUnitType
(
)
)
{
case
MODULE_DECLARATION
:
break
;
case
PACKAGE_DECLARATION
:
break
;
case
TYPE_DECLARATION
:
calculate
(
compilationUnit
,
compilationUnit
.
getDeclaredTypes
(
)
)
;
break
;
default
:
throw
new
SpoonException
(
"Cannot print compilation unit of type "
+
compilationUnit
.
getUnitType
(
)
)
;
}
}
