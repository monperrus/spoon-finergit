/** 
 * Write a pre unary operator.
 */
void
preWriteUnaryOperator
(
UnaryOperatorKind
o
)
{
switch
(
o
)
{
case
POS
:
write
(
"+"
)
;
break
;
case
NEG
:
write
(
"-"
)
;
break
;
case
NOT
:
write
(
"!"
)
;
break
;
case
COMPL
:
write
(
"~"
)
;
break
;
case
PREINC
:
write
(
"++"
)
;
break
;
case
PREDEC
:
write
(
"--"
)
;
break
;
}
}
