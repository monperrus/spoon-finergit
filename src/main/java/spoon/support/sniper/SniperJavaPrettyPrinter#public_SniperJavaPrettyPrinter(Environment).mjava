/** 
 * Creates a new  {@link PrettyPrinter} which copies origin sources and prints only changes.
 */
public
SniperJavaPrettyPrinter
(
Environment
env
)
{
super
(
env
)
;
inlineElseIf
=
false
;
env
.
useTabulations
(
true
)
;
env
.
setCommentEnabled
(
true
)
;
env
.
setOutputType
(
OutputType
.
COMPILATION_UNITS
)
;
mutableTokenWriter
=
new
MutableTokenWriter
(
env
)
;
setPrinterTokenWriter
(
createTokenWriterListener
(
mutableTokenWriter
)
)
;
this
.
setIgnoreImplicit
(
false
)
;
}
